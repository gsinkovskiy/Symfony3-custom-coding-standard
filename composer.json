{
    "name": "gsinkovskiy/itworks-coding-standard",
    "type": "coding-standard",
    "description": "Customized CodeSniffer ruleset for the ITWorks Soft projects",
    "keywords": ["Symfony3", "coding standard", "phpcs"],
    "homepage": "https://github.com/gsinkovskiy/itworks-coding-standard",
    "license": "MIT",
    "authors": [
        {
            "name": "Glib Synkovskyi",
            "email": "gleb.sinkovskiy@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ITWorks\\": "ITWorks/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "phacility/libphutil",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/phacility/libphutil.git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": ["src/"]
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "squizlabs/php_codesniffer": "~2.0",
        "phacility/libphutil": "dev-master"
    }
}
